{
  "version": 2,
  "builds": [
    {
      "src": "vercel_app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/vercel_app.py"
    }
  ],
  "env": {
    "ENVIRONMENT": "production"
  },
  "functions": {
    "vercel_app.py": {
      "maxDuration": 30
    }
  }
}